<exercise number="1">
    <statement><p>
        Solve.
        <ol label="a">
            <li><p><m>5-2(3x-4)=28-2x </m></p></li>
            <li><p><m>0.0048z-0.12=-0.08+0.0016z</m></p></li>
        </ol>
    </p></statement>
    <answer><p>
        <ol label="a">
            <li><p><m>\dfrac{-15}{4} </m></p></li>
            <li><p><m>12.5</m></p></li>
        </ol>
    </p></answer>
</exercise>

<exercise number="2">
