<exercise number="17"><statement><p><ol marker="a" cols="3">
    <li><m>\sqrt{4x^2}</m></li>
    <li><m>\sqrt{(x-5)^2}</m></li>
    <li><m>\sqrt{x^2-6x+9}</m></li></ol></p></statement>
    <answer><p><ol marker="a" cols="2">
        <li><m>2\abs{x}</m></li>
        <li><m>\abs{x-5}</m></li>
        <li><m>\abs{x-3}</m></li>
    </ol></p></answer></exercise>

